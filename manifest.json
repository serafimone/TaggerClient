{
  "name": "Getting Started Example",
  "version": "1.0",
  "description": "Build an Extension!",
  "browser_action": {
    "default_popup": "index.html"
  },
   "manifest_version": 2,
   "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
   "permissions": [
    "*://localhost:*/*",
    "contextMenus"	
   ],
   "background": {
     "scripts": ["background.js"]
   }
}